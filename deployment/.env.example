# Traefik Configuration
DOMAIN=yourdomain.com
CLOUDFLARE_EMAIL=your-email@example.com
CLOUDFLARE_DNS_API_TOKEN=your-cloudflare-api-token

# Traefik Dashboard Credentials
# Generate with: ./generate-traefik-auth.sh
# Or manually: echo $(htpasswd -nbB admin your-password) | sed -e s/\\$/\\$\\$/g
TRAEFIK_DASHBOARD_CREDENTIALS=admin:$$2y$$05$$example...hash...here

# South East Archers Configuration
SECRET_KEY=your-secret-key-here-generate-random-32-chars
MYSQL_ROOT_PASSWORD=strong-root-password
MYSQL_USER=sea_user
MYSQL_PASSWORD=strong-user-password
DATABASE_URL=mysql+pymysql://sea_user:strong-user-password@db:3306/southeastarchers

# Email Configuration
MAIL_SERVER=smtp.gmail.com
MAIL_PORT=587
MAIL_USE_TLS=True
MAIL_USERNAME=your-email@gmail.com
MAIL_PASSWORD=your-app-password
MAIL_DEFAULT_SENDER=noreply@southeastarchers.ie

# Payment Configuration
SUMUP_API_KEY=your-sumup-api-key
SUMUP_MERCHANT_CODE=your-merchant-code
